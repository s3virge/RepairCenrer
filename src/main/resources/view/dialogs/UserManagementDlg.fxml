<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="375.0" prefWidth="728.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.controllers.UserManagementDlgController">
   <children>
      <TableView fx:id="tvUserInfo" layoutX="36.0" layoutY="23.0" prefHeight="278.0" prefWidth="655.0">
        <columns>
          <TableColumn fx:id="colSurname" prefWidth="91.0" text="Фамилия" />
          <TableColumn fx:id="colName" prefWidth="93.0" text="Имя" />
            <TableColumn fx:id="colPatronymic" prefWidth="75.0" text="Отчество" />
            <TableColumn fx:id="colLogin" prefWidth="75.0" text="Логин" />
            <TableColumn fx:id="colPassword" prefWidth="75.0" text="Пароль" />
            <TableColumn fx:id="colEmail" prefWidth="75.0" text="E-mail" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <ButtonBar layoutX="36.0" layoutY="325.0" prefHeight="26.0" prefWidth="265.0">
        <buttons>
            <Button fx:id="btnAddUser" mnemonicParsing="false" onAction="#OnAddBtn" text="Добавить" />
            <Button mnemonicParsing="false" text="Изменить" />
            <Button mnemonicParsing="false" text="Удалить" />
        </buttons>
      </ButtonBar>
      <Button cancelButton="true" layoutX="598.0" layoutY="325.0" mnemonicParsing="false" onAction="#onBtnCancelClick" text="Отмена" />
   </children>
</AnchorPane>
